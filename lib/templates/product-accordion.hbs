<div class="accordion">
  {{#each products}}
  <div class="accordion-toggle" id="{{toggle_id}}">
    <span>{{title}}</span><i class="fas fa-chevron-down"></i>
  </div>
  <div class="accordion-content" id="{{toggle_id}}-content">
    <p><a href="{{permalink}}">View this information as a web page.</a></p>
    <h3>State Availability</h3>
    {{#if states_review_date}}<p class="review-date">Current as of {{states_review_date}}</p>{{/if}}
    <p>{{{states}}}</p>
    <h3>Plan Information</h3>
    {{#if desc_review_date}}<p class="review-date">Current as of {{desc_review_date}}</p>{{/if}}
    {{#if lower_issue_age}}<p>Issue ages {{lower_issue_age}}&ndash;{{upper_issue_age}}.</p>{{/if}}
    {{{description}}}
    {{#if medicare_product}}<p><em>Some information may vary by state. <a href="{{medicare_quote_engine_url}}">See state-specific information and rates</a>.</em></p>{{/if}}
    <div class="kit-request">
      <h3>Request a Product Kit for {{carriername}} {{title}}</h3>
      <p>Weâ€™ll email you a kit with brochures, commissions, and rates all specific to the state of your choice.</p>
      <p><a class="elementor-button" href="{{kit_request_url}}">Request a Kit</a></p>
    </div>
  </div>
  {{/each}}
</div>